<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <!-- sulu-admin -->
        <service id="sulu_redirect.admin" class="Sulu\Bundle\RedirectBundle\Admin\RedirectAdmin" public="false">
            <argument>%sulu_admin.name%</argument>

            <tag name="sulu.admin"/>
            <tag name="sulu.context" context="admin"/>
        </service>

        <service id="sulu.repository.redirect_route" class="Sulu\Bundle\RedirectBundle\Entity\RedirectRouteRepository">
            <factory service="doctrine.orm.entity_manager" method="getRepository"/>

            <argument type="string">SuluRedirectBundle:RedirectRoute</argument>
        </service>
    </services>
</container>
